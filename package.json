{
    "name": "nodast",
    "description": "Asterisk FastAgi Proxy",
    "version": "0.0.1",
    "author": "Pascal Deschenes <pdeschen@rassemblr.com> (http://blog.rassemblr.com)",
    "homepage": "http://github.com/pdeschen/nodast",
    "preferGlobal": true,
    "keywords": ["telephony", "asterisk", "proxy", "fastagi"],
    "directories": {
