        "ansi-color": "1",
        "node-syslog": "1"
    },
    "engines": {
        "node": "0.4 || 0.5",
        "npm": "1"
    },
    "licenses": [{
        "type": "MIT",
        "url": "http://github.com/pdeschen/nodast/raw/master/LICENSE"
